    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
